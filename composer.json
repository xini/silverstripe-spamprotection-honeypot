{
    "name": "innoweb/silverstripe-spamprotection-honeypot",
    "description": "Provides invisible honeypot spam protection for Silverstripe CMS.",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe",
        "spam",
        "protection",
        "honeypot",
        "form"
    ],
    "authors": [{
        "name": "Florian Thoma",
        "homepage": "https://www.innoweb.com.au"
    }],
    "homepage": "https://github.com/xini/silverstripe-spamprotection-honeypot",
    "require": {
        "silverstripe/framework": "^5",
        "silverstripe/spamprotection": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5"
    },
    "autoload": {
        "psr-4": {
            "Innoweb\\SpamProtectionHoneypot\\": "src"
            "Innoweb\\SpamProtectionHoneypot\\Tests\\": "tests"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
	}
}
